  "description": "News extension for T3STARTER Templates.",
  "homepage": "https://www.t3starter.de",
  "license": "GPL-2.0+",
  "keywords": [
    "TYPO3",
    "extension"
  ],
  "require": {
      "typo3/cms-core": "^12.4 || ^13.0",
      "typo3/cms-rte-ckeditor": "^12.4 || ^13.0",
