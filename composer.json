    "license": [
        "GPL-3.0-or-later"
    ],
    "homepage": "https://github.com/CMS-Internetsolutions/t3starter_news",
  "keywords": [
    "Bootstrap",
    "News"
  ],
  "require": {
      "typo3/cms-core": "^12.4 || ^13.0",
      "typo3/cms-rte-ckeditor": "^12.4 || ^13.0",
